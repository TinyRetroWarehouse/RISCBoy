#!/usr/bin/env python3

import sys

failed = False

tbman_lines = []

for line in open(sys.argv[1]).readlines():
	if line.startswith("TBMAN:"):
		tbman_lines.append(line.strip()[7:])

failed = failed or tbman_lines[0] != "Raising ECALL"
failed = failed or tbman_lines[1] != "Handling ECALL"
failed = failed or not tbman_lines[2].startswith("CPU requested termination")
failed = failed or tbman_lines[2].split(" ")[-1] != "0"

if not failed:
	print("Test PASSED.")